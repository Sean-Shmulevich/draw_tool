{
  "name": "draw-tool",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "build_deploy": "vite build && npx gh-pages -d dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:update": "playwright test --update-snapshots",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/p5": "^1.7.6",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.55.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "p5": "^2.0.5"
  }
}
